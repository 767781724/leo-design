{
  "name": "leo-design",
  "version": "1.0.94",
  "description": "React-based mobile style library",
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "lib/index.d.ts",
  "unpkg": "dist/index.min.js",
  "scripts": {
    "eslint": "eslint src --ext .ts,.tsx",
    "build:ts": "tsc -p ./tsconfig.build.json && cp -rf es lib",
    "build": "rimraf lib esm dist && yarn build:ts && gulp -f gulpfile.js",
    "server": "gulp watch -f gulpfile.js",
    "start": "npx dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run eslint"
    }
  },
  "keywords": [
    "react",
    "mobile",
    "design",
    "UI",
    "rollup"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/767781724/leo-design.git"
  },
  "author": "Leo",
  "license": "ISC",
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "peerDependencies": {},
  "dependencies": {
    "@ant-design/icons": "^4.4.0",
    "rc-notification": "^4.5.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-transition-group": "^4.4.1"
  },
  "sideEffects": [
    "dist/*",
    "es/**/style/*",
    "lib/**/style/*",
    "*.scss"
  ],
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/runtime": "^7.12.5",
    "@babel/runtime-corejs3": "^7.12.5",
    "@types/classnames": "^2.2.11",
    "@types/react": "^17.0.0",
    "@types/react-transition-group": "^4.4.1",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "@umijs/plugin-sass": "^1.1.1",
    "babel-loader": "^8.2.2",
    "classnames": "^2.2.6",
    "core-js": "^3.8.2",
    "dumi": "^1.1.3",
    "dumi-theme-mobile": "^1.0.4",
    "eslint": "^7.17.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-react": "7.21.1",
    "gh-pages": "^3.1.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-babel": "^8.0.0",
    "gulp-cssnano": "^2.1.3",
    "gulp-if": "^3.0.0",
    "gulp-ignore": "^3.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-uglify": "^3.0.2",
    "gulp-umd": "^2.0.0",
    "husky": "^4.3.7",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.4",
    "rimraf": "^3.0.2",
    "through2": "^4.0.2",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.1.3"
  }
